{
  "service": {
    "name": "nats",
    "tags": ["nats"],
    "port": <%= p("nats.port") %>,
    "check": {
      "id": "nats-check",
      "name": "nats check",
      "script": "nc -v -z -w 3 <%= spec.networks.send(properties.network).ip %> <%= p("nats.port") %>",
      "interval": "10s"
    }
  }
}
